df = pd.read_pickle('contracts_data.pkl')
df_prod = df['products.product']
df_reg = pd.DataFrame(df['_regNum'])
lst = []
for i in df_prod:
    for j in i:
        lst.append(j)
ops = []
final = []
for i in lst:
    ops.append(flatdict.FlatDict(i))
for i in ops:
    final.append(i)
ddf = pd.DataFrame(final)
